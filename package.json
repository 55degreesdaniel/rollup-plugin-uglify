{
  "name": "rollup-plugin-uglify",
  "version": "0.2.0",
  "description": "Rollup plugin to minify generated bundle",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c -f cjs -o dist/index.js",
    "lint": "eslint src test/*.js",
    "pretest": "npm run lint && npm run build",
    "test": "ava",
    "prepublish": "npm test"
  },
  "eslintConfig": {
    "extends": "postcss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrySound/rollup-plugin-uglify.git"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "uglify",
    "minify"
  ],
  "author": "Bogdan Chadkin <trysound@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrySound/rollup-plugin-uglify/issues"
  },
  "homepage": "https://github.com/TrySound/rollup-plugin-uglify#readme",
  "dependencies": {
    "uglify-js": "^2.6.1"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^1.10.3",
    "eslint-config-postcss": "^1.0.0",
    "rollup": "^0.25.2",
    "rollup-plugin-babel": "^2.3.9"
  }
}
